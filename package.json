{
  "name": "magicmirror",
  "version": "2.9.0",
  "description": "The open source modular smart mirror platform.",
  "main": "src/index.js",
  "scripts": {
    "start": "concurrently --kill-others npm:startonly npm:electron",
    "startonly": "react-scripts start",
    "build": "react-scripts build",
    "electron": "electron -r esm src/server/index.js",
    "lint": "eslint src/**/*.{js,jsx,ts,tsx}",
    "lint:fix": "eslint --fix src/**/*.{js,jsx,ts,tsx}",
    "postinstall": "cd vendor && npm install",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha tests --recursive",
    "test:unit": "NODE_ENV=test ./node_modules/mocha/bin/mocha tests/unit --recursive",
    "test:e2e": "NODE_ENV=test ./node_modules/mocha/bin/mocha tests/e2e --recursive",
    "config:check": "node tests/configs/check_config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshwilsonvu/MagicMirror.git"
  },
  "keywords": [
    "magic mirror",
    "smart mirror",
    "mirror UI",
    "modular"
  ],
  "authors": [
    "Michael Teeuw",
    "Josh Wilson"
  ],
  "contributors": [
    "https://github.com/joshwilsonvu/MagicMirror/graphs/contributors"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joshwilsonvu/MagicMirror/issues"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "codegen.macro": "^3.0.0",
    "concurrently": "^5.0.0",
    "current-week-number": "^1.0.7",
    "danger": "^3.1.3",
    "http-auth": "^3.2.3",
    "jsdom": "^11.6.2",
    "mocha": "^4.1.0",
    "mocha-each": "^1.1.0",
    "mocha-logger": "^1.0.6",
    "node-glob": "^1.2.0",
    "preval.macro": "^3.0.0",
    "spectron": "^3.8.0"
  },
  "dependencies": {
    "colors": "^1.1.2",
    "electron": "^3.0.13",
    "esm": "^3.2.25",
    "express": "^4.16.2",
    "express-ipfilter": "^1.0.1",
    "feedme": "latest",
    "helmet": "^3.21.2",
    "iconv-lite": "latest",
    "lodash": "^4.17.15",
    "moment": "latest",
    "nanoid": "^2.1.6",
    "nunjucks": "^3.2.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-scripts": "^3.2.0",
    "react-transition-group": "^4.3.0",
    "request": "^2.88.0",
    "roboto-fontface": "^0.10.0",
    "rrule": "^2.6.2",
    "rrule-alt": "^2.2.8",
    "simple-git": "^1.85.0",
    "socket.io": "^2.1.1",
    "valid-url": "latest"
  },
  "browserslist": [
    "last 1 chrome version"
  ]
}
